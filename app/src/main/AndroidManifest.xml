<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.bookonline">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.BookOnline">
        <activity android:name=".ui.category.CategoryActivity"></activity>
        <activity android:name=".LoginActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".SaleOrderActivity"
            android:theme="@style/AppTheme" />
        <activity
            android:name=".UserofAdmin"
            android:theme="@style/AppTheme" />
        <activity android:name=".Admin_Nav_Bottom" />
        <activity
            android:name=".PayActivity"
            android:theme="@style/AppTheme" />
        <activity android:name=".ui.category.ProductDetails" />
        <activity
            android:name=".Register"
            android:theme="@style/AppTheme" />
        <activity
            android:name=".ui.profile.EditProfileActivity"
            android:theme="@style/AppTheme" />
        <activity
            android:name=".ui.history.PurchaseHistoryActivity"
            android:theme="@style/AppTheme" />
        <activity
            android:name=".ui.profile.ChangePassActivity"
            android:theme="@style/AppTheme" />
        <activity android:name=".NavBottom" />
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name"
            android:theme="@style/AppTheme" />
    </application>

</manifest>